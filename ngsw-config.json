{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/pwa",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/manifest.webmanifest", "/**/*.css", "/**/*.js"]
      }
    }
  ],
  "navigationUrls": ["/**", "!/api/**", "!/thumbnail/**"]
}
